{
    "manifest_version": 3,
    "name": "Catloaf, where are we?",
    "version": "0.0.1.1",
    "description": "Ask the loafing cat where you are and he'll meow the page title at you.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        }
    },
    "permissions": [
        "tabs"
    ]
}